        {
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper": {
              "& when not (@transparentBg), (@showBorders)": {
                "box-shadow": "2px 15px 50px rgba(41, 44, 61, 0.1)",
                "-webkit-box-shadow": "2px 15px 50px rgba(41, 44, 61, 0.1)",
                "transition": "box-shadow 0.1s",
                "-webkit-transition": "box-shadow 0.1s",
                "-o-transition": "box-shadow 0.1s",
                "-moz-transition": "box-shadow 0.1s",
                "&:hover": {
                  "box-shadow": "2px 21px 50px rgba(41, 44, 61, 0.15)",
                  "-webkit-box-shadow": "2px 15px 50px rgba(41, 44, 61, 0.15)"
                }
              },
              "& when (@showBorders)": {
                "border": "1px solid @bordersColor",
                "& when (@transparentBg) and not (@flexDirection)": {
                  "border": "none",
                  "position": "relative",
                  "&, &:hover": {
                    "box-shadow": "none",
                    "-webkit-box-shadow": "none"
                  },
                  ".card-box": {
                    "padding": "0"
                  },
                  "&:after": {
                    "content": "\"\"",
                    "position": "absolute",
                    "bottom": "-1.5rem",
                    "width": "100%",
                    "border-bottom": "1px solid @bordersColor",
                    "@media (max-width: 767px)": {
                      "bottom": "-1rem"
                    }
                  }
                }
              },
              "& when not (@transparentBg)": {
                "background": "@cardBg"
              },
              "@media (max-width: 767px)": {
                "flex-wrap": "wrap"
              }
            },
            ".card-box": {
              "width": "100%"
            },
            "& when (@checkImages) and not (@flexDirection)": {
              ".card-img": {
                "padding": "0",
                "@media (max-width: 991px)": {
                  "width": "100%",
                  "margin-bottom": "2rem"
                },
                "& when not (@chessStyle)": {
                  "@media (min-width: 992px)": {
                    "& when not (@reverseContent)": {
                      "margin-right": "2rem"
                    },
                    "& when (@reverseContent)": {
                      "margin-left": "2rem"
                    }
                  }
                }
              },
              "& when (@chessStyle)": {
                "@media (min-width: 992px)": {
                  ".mbr-row .card": {
                    "& when not (@reverseContent)": {
                      "&:nth-child(odd) .card-img": {
                        "margin-left": "2rem"
                      },
                      "&:nth-child(even) .card-img": {
                        "margin-right": "2rem"
                      }
                    },
                    "& when (@reverseContent)": {
                      "&:nth-child(odd) .card-img": {
                        "margin-right": "2rem"
                      },
                      "&:nth-child(even) .card-img": {
                        "margin-left": "2rem"
                      }
                    }
                  }
                }
              }
            },
            ".line": {
              "&:after": {
